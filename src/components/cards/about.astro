---
export interface Props {
  image: string;
}
const {  image } = Astro.props;
---

<div
  class=" flex flex-col items-center p-5 sm:p-6 lg:p-8 rounded-3xl border border-box-border bg-box-bg shadow-lg shadow-box-shadow relative overflow-hidden"
>
  <div class="flex justify-center  w-full max-w-[250px] overflow-hidden rounded-full">
    <img src={image} alt="imagen del integrante" />
  </div>
</div>
